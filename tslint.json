{
    "rules": {
        "class-name": true,
        "no-var-keyword": true,
        "no-use-before-declare": false,
        "no-console": [
            true
        ],
        "no-reference": true,
        "no-internal-module": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-invalid-this": [
            true,
            "check-function-in-method"
        ],
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "triple-equals": [
            true,
            "allow-undefined-check",
            "allow-null-check"
        ],
        "switch-default": false,
        "use-isnan": true,
        "max-line-length": [
            true,
            120
        ],
        "max-file-line-count": [
            true,
            500
        ]
    }
}
